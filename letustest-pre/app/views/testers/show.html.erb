<% provide(:title, @tester.fname) %>

<div class="row">
  <div class="span3">
    <div class="box round-corners box-shadow" id="t-stats">

      <h1> Welcome, <%= @tester.fname %> </h1>

      E-mail: <%= @tester.email %> <br/>

      Total Score: <%= @getTotalScore %> <br/>

    </div>

    <div class="box round-corners box-shadow" id="t-stats">
      <div class="box-title"><h2>Statistics</h2></div>
      <div class="box-content">
        <p><span class="badge"><%= @getTotalScore %></span> Testlio score <a href="#"><i class="icon-gray icon-question-sign"></i></a></p>
        <p><span class="badge">1026</span> profile views</p>
      </div>
    </div>

    <div class="box round-corners box-shadow" id="t-stats">
      <div class="box-title"><h2>Verification</h2></div>
      <div class="box-content">
        <ul class="unstyled report">
          <li class="clearfix report-row">
            <i class="icon-facetime-video icon-gray"></i>Facebook Connected<i class="icon-ok-green pull-right"></i>
            <div class="report-row-actions">282 Friends</div>
          </li>
          <li class="clearfix report-row"><i class="icon-book icon-gray"></i>LinkedIn Connected<i class="icon-ok-green pull-right"></i>
            <div class="report-row-actions">Validated</div>
          </li>
        </ul>
      </div>
    </div>

    <%= link_to "EDIT PROFILE", edit_tester_path, class: "profile-btn" %>
    <%= link_to "PROJECTS", projects_tester_path, class: "profile-btn" %>

    <ul class="nav nav-list">
      <li class="nav-header">List header</li>
      <!--<li class="active"><a href="#">Home</a></li>-->
      <li class="<%= 'active' if current_page?(projects_tester_path) %>"><%= link_to "PROJECTS", projects_tester_path %></li>
      <!--<li><a href="#">Library</a></li>-->
      <!--<li><a href="#">Applications</a></li>-->
      <li class="nav-header">Another list header</li>
      <li class="<%= 'active' if current_page?(current_tester) %>"><%= link_to "DASHBOARD", current_tester %></li>
