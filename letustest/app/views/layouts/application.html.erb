<!DOCTYPE html>
<html>
<head>
	<title>Letustest</title>
	<%= stylesheet_link_tag    "application", :media => "all" %>
	<%= javascript_include_tag "application" %>
	<%= csrf_meta_tags %>
</head>
<body>
	<div class="container">
		<%# flash messages %>
		<% flash.each do |key, value| %>
		<div class="alert alert-<%= key %>"><%= value %></div>	
	
	<% end %>
	<div class="row">
	<div class="span9">
	<%= yield %>
	</div>
	</div>
	</div>
	<%= debug(params) if Rails.env.development? %>
</body>
</html>
