<h1>Listing feedbacks</h1>

<table class="table">
  <tr>
    <th>URL</th>
    <th>Questions</th>
    <th>Email Address</th>
    <th>Number of Bugs</th>
    <th>Due date</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Telephone</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feedbacks.each do |feedback| %>
  <tr>
    <td><%= feedback.url %></td>
    <td><%= feedback.questions %></td>
    <td><%= feedback.email %></td>
    <td><%= feedback.numofbugs %></td>
    <td><%= feedback.duedate %></td>
    <td><%= feedback.firstname %></td>
    <td><%= feedback.lastname %></td>
    <td><%= feedback.telephone %></td>
    <td><%= link_to 'Show', feedback %></td>
    <td><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
    <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feedback', new_feedback_path %>
